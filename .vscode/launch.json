    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp2.1/TAiO.Subgraphs.dll",
            // "args": ["Data/K-80-1.csv", "Data/K-80-2.csv"],
            // "args": ["Data/Vismara-1.csv", "Data/Vismara-2.csv"],
            // "args": ["Data/3-3 vertex complete same graphs/3 vertex complete.csv", "Data/3-3 vertex complete same graphs/3 vertex complete.csv"],
            // "args": ["Data/6-4 subgraph 4/4v4e.csv", "Data/6-4 subgraph 4/6v.csv"],
            "args": ["Data/Complete-of-5-vertices.csv", "Data/Complete-of-6-vertices.csv"],
            // "args": ["Data/Circles-of-5-and-6-vertices-1.csv", "Data/Circles-of-5-and-6-vertices-2.csv"],
            // "args": ["Data/Line-1.csv", "Data/Line-2.csv"],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}